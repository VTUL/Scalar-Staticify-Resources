# Location: /migrant-works-of-the-virginia-tidewater/.htaccess
RewriteEngine On
RewriteBase /
# Redirect / to /index
RewriteRule ^/?$ /migrant-works-of-the-virginia-tidewater/index [L,R=302,NC]

# Load HTML files without extension
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.+)$ /migrant-works-of-the-virginia-tidewater/$1.html [L,QSA]
